<svg width="400" height="100" viewBox="0 0 400 100" xmlns="http://www.w3.org/2000/svg">
  <!-- IBMP Official Logo -->
  <defs>
    <!-- Gradient for shield -->
    <linearGradient id="shieldGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2C5D7A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1E3A5F;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Circular Badge -->
  <circle cx="50" cy="50" r="45" fill="#2C5D7A" stroke="#1E3A5F" stroke-width="2"/>
  
  <!-- Shield Shape -->
  <path d="M50 10 L80 20 L80 65 C80 75 65 85 50 90 C35 85 20 75 20 65 L20 20 Z" 
        fill="url(#shieldGradient)" stroke="white" stroke-width="2"/>
  
  <!-- Medical Cross -->
  <rect x="46" y="30" width="8" height="30" fill="white"/>
  <rect x="35" y="44" width="30" height="8" fill="white"/>
  
  <!-- Caduceus Symbol -->
  <g transform="translate(40,25)">
    <circle cx="10" cy="10" r="3" fill="none" stroke="white" stroke-width="1.5"/>
    <path d="M10 7 Q15 10 10 13 Q5 10 10 7" fill="none" stroke="white" stroke-width="1"/>
  </g>
  
  <!-- Text around circle -->
  <path id="topCircle" d="M 15,50 A 35,35 0 0,1 85,50" fill="none"/>
  <text font-family="Arial, sans-serif" font-size="8" font-weight="bold" fill="white">
    <textPath href="#topCircle" startOffset="10%">INTERNATIONAL BOARD OF MEDICAL PRACTITIONERS</textPath>
  </text>
  
  <path id="bottomCircle" d="M 85,50 A 35,35 0 0,1 15,50" fill="none"/>
  <text font-family="Arial, sans-serif" font-size="6" font-weight="normal" fill="white">
    <textPath href="#bottomCircle" startOffset="25%">ESTABLISHED SINCE 2020</textPath>
  </text>
  
  <!-- Large IBMP Text -->
  <text x="120" y="45" font-family="Arial, sans-serif" font-size="48" font-weight="900" fill="#2C5D7A">IBMP</text>
  
  <!-- Subtitle -->
  <text x="120" y="65" font-family="Arial, sans-serif" font-size="12" font-weight="normal" fill="#5A7A95">
    International Board of Medical Practitioners
  </text>
  
  <text x="120" y="80" font-family="Arial, sans-serif" font-size="10" font-weight="normal" fill="#7A95B5">
    Excellence in Medical Education â€¢ Established Since 2020
  </text>
</svg>
